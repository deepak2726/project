<h1>Listing questions</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Question type</th>
    <th>Sort order</th>
    <th>Is published</th>
    <th>Is shared boolean</th>
    <th>User</th>
    <th>Subject</th>
    <th>Name</th>
    <th>Parent</th>
    <th>Category subject</th>
    <th>Mark</th>
    <th>Feed back</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @questions.each do |question| %>
  <tr>
    <td><%= question.description %></td>
    <td><%= question.question_type_id %></td>
    <td><%= question.sort_order %></td>
    <td><%= question.is_published %></td>
    <td><%= question.is_shared %></td>
    <td><%= question.user_id %></td>
    <td><%= question.subject_id %></td>
    <td><%= question.name %></td>
    <td><%= question.parent_id %></td>
    <td><%= question.category_subject_id %></td>
    <td><%= question.mark %></td>
    <td><%= question.feed_back %></td>
    <td><%= link_to 'Show', question %></td>
    <td><%= link_to 'Edit', edit_question_path(question) %></td>
    <td><%= link_to 'Destroy', question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Question', new_question_path %>
